    "require-dev": {
        "geerlingguy/drupal-vm": "~4.3"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": false,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "config-entity-save-exception-workaround": "https://www.drupal.org/files/issues/2018-09-26/2925890-10.patch"
            },
            "drupal/address": {
                "2668006 - Provide logical access control for when IPE should be applied to moderated nodes": "https://www.drupal.org/files/issues/address-search-api-full-name-properties-2812659-28.patch"
            },
            "drupal/ds": {
                "do-not-block-layout-settings": "./patches/ds/custom/do-not-block-layout-settings.patch"
            },
            "drupal/entityform_block": {
                "entityform-block-form-mode": "./patches/entityform_block/custom/entityform-block-form-mode.patch"
            },
            "drupal/votingapi": {
                "vote-context": "./patches/votingapi/custom/votingapi-entity-context.patch",
                "vote-views-integration": "./patches/votingapi/custom/votingapi-views-integration.patch"
            },
            "drupal/vote_up_down": {
                "vote-context": "./patches/vote_up_down/custom/votingapi-entity-context.patch",
                "vote-ajax-api-instead-of-json": "./patches/vote_up_down/custom/ajax-api-instead-of-custom-json.patch"
            },
            "drupal/bootstrap": {
                "use-fa-instead-of-glyphicons": "./patches/bootstrap/custom/use-fa-instead-of-glyphicons.patch"
            },
            "drupal/bootstrap_layouts": {
                "layout-link-wrapper": "./patches/bootstrap_layouts/custom/layout-link-wrapper.patch"
            },
            "drupal/similarterms": {
                "no-tags-bypass-filter-config": "./patches/similarterms/custom/no-tags-bypass-filter-config.patch",
                "fix-php72-compat": "https://www.drupal.org/files/issues/query_4.patch"
            },
            "drupal/blazy": {
                "blazy-media-4x-support": "./patches/blazy/contrib/2881849-8.patch"
            },
            "drupal/rules": {
                "entityqueue-serialization-in-container-error": "./patches/rules/contrib/2803823.patch",
                "2951263-rules-13": "./patches/rules/contrib/2951263-rules-13.patch"
            },
            "drupal/font_field_icon": {
                "change-font-awesome-css-path": "./patches/font_field_icon/custom/change-font-awesome-css-path.patch"
            },
            "drupal/panels": {
                "panels-block-attributes": "./patches/panels/custom/panels_custom_attributes_in_panels_blocks-2849867-21.patch"
            },
            "drupal/better_exposed_filters": {
                "add-layout-options": "./patches/better_exposed_filters/custom/add-layout-options.patch"
            },
            "drupal/views_ajax_history": {
                "views_ajax_history-configure_disable-2923471-3": "./patches/views_ajax_history/contrib/views_ajax_history-configure_disable-2923471-3.patch"
            },
            "drupal/realname": {
                "realname-extra-field-label-config": "./patches/realname/custom/realname-extra-field-label-config.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    }
}
